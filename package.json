{
  "name": "tesi-alessandro-serafini-ws",
  "version": "1.0.0",
  "description": "TesiAlessandroSerafiniWS",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "posttest": "npm run lint",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "dev": "npm run clean && nodemon server.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^4.2.7",
    "@loopback/authentication-passport": "^2.1.7",
    "@loopback/boot": "^2.3.3",
    "@loopback/context": "^3.9.0",
    "@loopback/core": "^2.8.0",
    "@loopback/openapi-v3": "^3.4.3",
    "@loopback/repository": "^2.7.0",
    "@loopback/rest": "^5.1.1",
    "@loopback/rest-explorer": "^2.2.4",
    "@loopback/service-proxy": "^2.3.2",
    "bcryptjs": "^2.4.3",
    "firebase-admin": "^8.12.1",
    "loopback-connector-mysql": "^5.4.3",
    "moment": "^2.26.0",
    "nodemailer": "^6.4.8",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.4.3",
    "source-map-support": "^0.5.19",
    "@loopback/testlab": "^3.1.7",
    "@types/node": "^10.17.26",
    "@typescript-eslint/parser": "^2.31.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@loopback/eslint-config": "^8.0.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^7.0.0",
    "typescript": "~3.9.5"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/"
    ],
    "ignore": [
      "dist/*"
    ],
    "ext": "ts",
    "exec": "npm run clean && npm start"
  }
}
